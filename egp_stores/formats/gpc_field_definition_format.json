{
    "ggc_only": {
        "default": false,
        "meta": {
            "description": "True if the field is only in gGC's"
        },
        "type": "boolean"
    },
    "indexed": {
        "default": false,
        "meta": {
            "description": "True if the field is must be indexed (defined for <50% of GC's and >4 byte object size)"
        },
        "type": "boolean"
    },
    "init_only": {
        "default": false,
        "meta": {
            "description": "True if the field is does not need to be stored in the GPC (is only used at initialization)"
        },
        "type": "boolean"
    },
    "pgc_only": {
        "default": false,
        "meta": {
            "description": "True if the field is on in pGC's"
        },
        "type": "boolean"
    },
    "reference": {
        "default": false,
        "meta": {
            "description": "True if the field is a reference"
        },
        "type": "boolean"
    },
    "signature": {
        "default": false,
        "meta": {
            "description": "True if the field is a signature"
        },
        "type": "boolean"
    }
}